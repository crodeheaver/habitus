{{outlet}}
<h1 class='title is-1'>List of Users {{#link-to 'admin.users.new' class="button is-primary"}}
  Add New
{{/link-to}}</h1>

{{#each model as |user|}}
  {{#content-card
  isEmpty=false
  title=user.displayName}}
  
  {{/content-card}}
  
{{else}}
  {{#content-card
  isEmpty=true
  title='No Existing Users'
  ActionLink='admin.users.new'}}
    <p>Since you don't have any existing Users, you'll need to create one to get started.</p>
  {{/content-card}}  
{{/each}}